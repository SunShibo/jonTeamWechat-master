@import '/components/wxParse/wxParse.wxss';

.body {
  background: #F2F3F8;
  overflow: hidden;
}

.main {
  width: 654rpx;
  margin: 24rpx;
  padding: 24rpx;
  background: #FFF;
  border-radius: 4rpx;
  display: flex;
  flex-direction: column;
}

.main-img {
  margin-bottom: 32rpx;

}

.out {
  font-size: 0
}

.font-one {
  font-size: 36rpx;
  color: #3E404B;
  margin-bottom: 16rpx;
  font-weight: bold
}

.font-two {
  display: inline-block;
  font-size: 24rpx;
  color: #AEAFBA;
  padding-left: 40rpx;
  background: url("data:image/png;base64,") no-repeat left center / auto 100%
}

.font-three {
  display: inline-block;
  font-size: 24rpx;
  color: #AEAFBA;
  margin: 0rpx 32rpx;
}


.font-four {
  margin: 20rpx 20rpx 0rpx 0rpx;
  display: inline-block;
  font-size: 24rpx;
  color: #7A7D90;
  border: 2rpx solid #C9CBD7;
  border-radius: 6rpx;
  padding: 0rpx 8rpx;
}

.fontFourLabel {
  display: flex;
  justify-content: start;
  flex-wrap: wrap;
  margin-bottom: 20rpx;
}

.content {
  color: #696C7F;
  font-size: 28rpx;
  margin-bottom: 112rpx;
  padding-top: 50rpx;
}