.body {
  width: 100%;
  min-height: 100vh;
  overflow: hidden;
  background: #F2F3F8;
  font-family: PingFangSC-Semibold, PingFang SC;
}

.main {
  background: #FFF;
  margin: 24rpx;
  padding: 24rpx;
}

.title {
  color: #3E404B;
  font-size: 36rpx;
  font-weight: 600;
  margin: 40rpx 0rpx 24rpx 0rpx;
}

.text {
  color: #696C7F;
  font-size: 28rpx;
}




.head {
  border-bottom: 2rpx solid #F2F3F8;
  width: 654rpx;
}

.head>view {
  display: inline-block;

}

.headTitle {
  margin-left: 26rpx;
}

.headTitleName {
  width: 150rpx;
  height: 36rpx;
  color: #3E404B;
  font-weight: 600;
  font-size: 36rpx;
  margin-bottom: 8rpx;
}

.headTitleVersions {
  width: 170rpx;
  height: 40rpx;
  color: #7A7D90;
  font-size: 32rpx;
  margin-bottom: 40rpx;
}

.contentOne {
  border-bottom: 2rpx solid #F2F3F8;
}

.contentOneImg {
  margin-bottom: 24rpx;
}

.contentOneImg>image {
  width: 327px;
  height: 184px;
}

.contentOneText {
  margin-bottom: 40rpx;
}

.contentThreeText {
  width: 327px;
  height: 1340px;
  margin-bottom: 16rpx;
  font-size: 14px;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(62, 64, 75, 1);
  line-height: 20px;
}

.contentTwoTextTitle>view {
  display: inline-block;
}



.dot {
  height: 0rpx;
  width: 0rpx;
  margin: 12rpx 16rpx 12rpx 0rpx;
  border: 6rpx solid rgb(32, 240, 255);
  border-radius: 50%;
}

.LittleTitle {
  height: 16px;
  font-size: 14px;
  font-weight: 500;
  color: rgba(62, 64, 75, 1);
}

.LittleText {
  height: auto;
  margin-top: 12rpx;
  margin-bottom: 28rpx;
  font-size: 14px;
  font-weight: 400;
  color: rgba(105, 108, 127, 1);
}