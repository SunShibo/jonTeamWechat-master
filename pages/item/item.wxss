.body {
  background: #F2F3F8;
  padding-bottom: 128rpx;
  min-height: 100vh;
}

.title {
  height: 96rpx;
  width: 750rpx;
  background: white;
  display: flex;
  align-items: center;
  justify-content: space-around;
  color: #696C7F;

}

.title>view {
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  border-bottom: 6rpx solid transparent;
}

.title .active {
  color: #3E404B;
  border-bottom: 6rpx solid #3F58BE;
}



.main {
  margin: 24rpx;
  background: #F2F3F8;
}

.block {
  background: white;
  padding: 24rpx;
  height: 220rpx;
  margin-bottom: 24rpx;
}

.block>view {
  display: inline-block;
}

.fontOne {
  width: 438rpx;
  font-size: 36rpx;
  color: #3E404B;
  margin-bottom: 32rpx;
  overflow: hidden;/*超出部分隐藏*/
  white-space: nowrap;/*不换行*/
  text-overflow: ellipsis;/*超出部分文字以...显示*/
}

.fontTwo {
  font-size: 24rpx;
  color: #7A7D90;
  margin-bottom: 16rpx;
}

.fontThree {
  font-size: 28rpx;
  color: #3E404B;
  margin-bottom: 8rpx;
}

.fontFour {

  height: 28rpx;
  font-size: 24rpx;
  color: #3E404B;
}

.imgOne {
  height: 180rpx;
  width: 180rpx;
  margin-left: 24rpx;
}

.achiFontOne {
  width: 438rpx;
  font-size: 36rpx;
  color: #3E404B;
  margin-bottom: 28rpx;
}

.achiFontTwo {
  font-size: 24rpx;
  color: #3E404B;
  margin-bottom: 18rpx;
  display: inline-block;
}

.gradeStar {
  display: inline-block;
  margin-left: 14rpx;
}

.gradeStar>view {
  display: inline-block;
}

.gradeStar image {
  width: 24rpx;
  height: 22rpx;
}

.commit {
  font-size: 24rpx;
  color: #3E404B;
  display: flex;
  width: 400rpx;
}

.achiFontThree {
  font-size: 24rpx;
  color: #3E404B;
  margin-bottom: 18rpx;
  display: inline-block;
}


