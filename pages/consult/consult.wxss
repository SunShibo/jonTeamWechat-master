.html{
  width: 100%;
  height:100vh;
}

.body{
  background: #F2F3F8;
  height:100vh;
  width: 100%;
  padding: 24rpx;
}

.main{
  background: #FFF;
  width:702rpx;
}

.head{
  width:702rpx;
  height:100rpx;
  background:#FFF;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.head>view{
  height: 36rpx;
  width: 144rpx;
  font-size: 36rpx;
  color: #3E404B;
  margin: 24rpx 0rpx;
  padding-left:24rpx ;
  border-left: 6rpx solid #3F58BE;
}
.block{
  /* background: white; */
  padding: 32rpx 0rpx;
  margin: 0rpx 24rpx;
  width: 654rpx;
  border-top: 2rpx solid #F2F3F8;
  font-size: 0;
}
.block>view{
  display: inline-block;
  position: relative;
  margin-top: -10rpx;
  height: 180rpx;
  width:450rpx;
}
.fontOne{
  height:40rpx;
  font-size:32rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  color:#3E404B;
  line-height:40rpx;
}
.fontTwo{
  font-size: 28rpx;
  color: #696C7F;
  margin: 16rpx 0rpx 0rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  overflow: hidden;display: -webkit-box;-webkit-line-clamp: 2;-webkit-box-orient: vertical;
  line-height: 36rpx;
  height:72rpx;

}
.out{
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-top: 10rpx;
}
.fontThree{
  box-sizing: border-box;
  flex:0 0 50%;
  white-space: nowrap;overflow: hidden;text-overflow: ellipsis;
  font-size: 24rpx;
  color: #AEAFBA;
  display: inline-block;
  padding-left: 40rpx;
  background: url("data:image/png;base64,") no-repeat left center / auto 100%
}
.fontFour{
  box-sizing: border-box;
  flex:0 0 50%;
  white-space: nowrap;overflow: hidden;text-overflow: ellipsis;
  font-size: 24rpx;
  color: #AEAFBA;
  display: inline-block;
}

.imgOne{
  height: 180rpx;
  width: 180rpx;
  margin-right: 24rpx;
  display: inline-block;
}
