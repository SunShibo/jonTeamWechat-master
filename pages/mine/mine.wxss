.body {
  background: #F2F3F8;
  height: 100vh;
}

.top {
  position: relative;
  height: 360rpx;
}

.beijing {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 360rpx;
}

.top text {
  display: block;
  color: #ffffff;
  position: absolute;
  z-index: 1;
  text-align: center;
  left: 0;
  top: 0;
  width: 100%;
}
.company{
  position: relative;
  right: 40rpx;
}
.via {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  position: absolute;
  top: 280rpx;
  width: 100%;
  z-index: 5;
}

.viaImg {
  height: 160rpx;
  width: 160rpx;
  border: 4rpx solid #F2F3F8;
  border-radius: 50%;
}

.name {
  color: #3E404B;
  font-size: 36rpx;
  text-align: center;
  width: 100%;
  font-family: PingFangSC-Semibold, PingFang SC;
  margin: 16rpx 0px 32rpx 0rpx;
}

.list {
  width: 702rpx;
  margin: 178rpx 24rpx 0rpx 24rpx;
  background: #FFF;
}

.list-item {
  height: 112rpx;
  width: 702rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  position: relative;
}

.line>view {
  font-size: 32rpx;
  color: #3E404B;
  font-family: PingFangSC-Regular, PingFang SC;
  display: inline-block;
  position: absolute;
  top: 34rpx;
}

.icon {
  height: 40rpx;
  width: 40rpx;
  margin: 0rpx 24rpx;
}

.back {
  height: 20rpx;
  width: 10rpx;
  position: absolute;
  right: 32rpx;
  top: 46rpx;
}

.line {
  width: 614rpx;
  height: 112rpx;
  border-bottom: 2rpx solid #F2F3F8;
}

.out {
  color: #F86F3E;
  background: #FFF;
  height: 112rpx;
  width: 702rpx;
  margin-top: 24rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}