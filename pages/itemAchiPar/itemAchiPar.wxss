.body {
  background: #F2F3F8;
  overflow: hidden;
  min-height: 100vh;
  width: 100%;
}

.main {
  margin: 24rpx;
}

.block {
  background: white;
  padding: 24rpx;
  height: 192rpx;
  margin-bottom: 24rpx;
  position: relative;
}

.block>view {
  position: absolute;
  width: 438rpx;
  display: inline-block;
}

.fontOne {
  width: 438rpx;
  font-size: 36rpx;
  color: #3E404B;
  margin: 8rpx 0rpx 32rpx 0rpx;
}

.fontTwo {
  width: 158rpx;
  font-size: 24rpx;
  color: #7A7D90;
  margin-bottom: 32rpx;
}

.fontThree {
  width: 166rpx;
  font-size: 24rpx;
  color: #7A7D90;
  position: absolute;
}

.imgOne {
  height: 192rpx;
  width: 192rpx;
  margin-right: 24rpx;
}



.item-grade {
  background: #FFF;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}


.comment1-description {
  display: flex;
  flex-direction: row;
}

.stars {
  background: url(https://oopsstatic.oss-cn-beijing.aliyuncs.com/1590130969257.png);
  width: 50rpx;
  height: 50rpx;
  background-size: 50rpx 50rpx;
  margin-left: 30rpx;
  transition: 600ms;
}

.half {
  background: url(https://oopsstatic.oss-cn-beijing.aliyuncs.com/1590130961116.png);
  width: 50rpx;
  height: 50rpx;
  background-size: 50rpx 50rpx;
  transition: 1s;
  transform: scale(1, 1)
}

.on {
  background: url(https://oopsstatic.oss-cn-beijing.aliyuncs.com/1590130932068.png);
  width: 50rpx;
  height: 50rpx;
  background-size: 50rpx 50rpx;
  transition: 1s;
  transform: scale(1, 1)
}

.gradeStar {
  display: inline-block;
}

.gradeStar>view {
  display: inline-block;
}

.gradeStar image {
  width: 54px;
  height: 48px;
}


.font-four {
  margin: 48rpx 0rpx 40rpx 0rpx;

}

.img-star {
  margin-bottom: 24rpx;
}

.img-star>image {
  height: 40rpx;
}

.textGrade {
  height: 326rpx;
  width: 630rpx;
  margin: 24rpx;
  padding: 24rpx;
  background: #F2F3F8;
}
.textBlock{
  width: 630rpx;
}

.btn{
  width: 702epx;
  height: 112rpx;
  display: flex;
  font-size:32rpx;
  flex-direction: column-reverse;
  justify-content: center;
  align-items: center;
  background: #3F58BE;
  color: #FFF;
  margin:378rpx 24rpx 48rpx 24rpx ;
}